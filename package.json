{
    "name": "@akutishevsky/lunchmoney-mcp",
    "version": "1.0.1",
    "description": "Model Context Protocol server for LunchMoney personal finance management",
    "main": "build/index.js",
    "bin": {
        "lunchmoney-mcp": "./build/index.js"
    },
    "scripts": {
        "build": "tsc && chmod 755 build/index.js",
        "build:dxt": "npm run build && mkdir -p dist && npx dxt pack . dist/lunchmoney.dxt",
        "prepublishOnly": "npm run build",
        "dev": "LUNCHMONEY_API_TOKEN='YOUR_TOKEN_HERE' npx @modelcontextprotocol/inspector node build/index.js"
    },
    "files": [
        "build"
    ],
    "keywords": [
        "mcp",
        "lunchmoney",
        "finance",
        "model-context-protocol",
        "ai",
        "assistant"
    ],
    "author": "",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/akutishevsky/lunchmoney-mcp.git"
    },
    "homepage": "https://github.com/akutishevsky/lunchmoney-mcp#readme",
    "type": "module",
    "dependencies": {
        "@modelcontextprotocol/sdk": "^1.17.0",
        "zod": "^3.25.76"
    },
    "devDependencies": {
        "@types/node": "^24.1.0",
        "typescript": "^5.8.3"
    }
}
